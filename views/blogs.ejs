    <div style="border-radius: 7% 0% 0% 7%;" class="menu-right max-md:hidden md:flex p-2 border-[2px] border-[#D7D1C6] shadow-xl  w-[0vw] h-[85%] bottom-0 bg-[#F2EFE9] fixed bottom-0 right-[-5%] overflow-hidden z-[9999999999999999]">
        <img class="w-full rounded-l-[6.5%] h-full object-cover object-center" src="/images/menuimage.avif" alt="">
    </div>
   <div  id="main"  style="font-family: headb;" class="overflow-x-hidden bg-[#D7D1C6] text-[#332E24]">
       <h1 class="text-8xl text-[#322D23] text-center mt-[3.5vw]">Blogs</h1>
       <div class="mx-auto pt-[5vw] min-h-[100vh] px-4">
        <div class="w-[90%] md:w-[70%] mb-[6vw] mx-auto h-fit border-2 border-[#322D23] rounded-lg p-5">
            <% if (blogs.length > 0) { %>
                <% blogs.forEach((blog) => { %>
                    <a href="/blogdets/<%= blog._id %>">
                        <div class="h-auto md:h-[48vh] shadow-md rounded-lg bg-[#F2EFE9] shadow-lg overflow-hidden mb-6 flex flex-col md:flex-row">
                            <!-- Blog Image -->
                            <img src="/images/uploads/<%= blog.coverImage.url %>" 
                                alt="Blog Image" 
                                class="w-full md:w-1/3 object-cover h-[200px] md:h-auto">
                            
                            <!-- Blog Content -->
                            <div class="p-5 flex flex-col justify-between w-full">
                                <div>
                                    <!-- Author Section -->
                                    <div class="flex items-center text-gray-800 text-xs md:text-sm space-x-2">
                                        <img class="w-8 h-8 md:w-[2vw] md:h-[2vw] rounded-full object-cover" 
                                             src="/images/menuimage2.avif" alt="">
                                        <span class="text-sm md:text-[1.5vw]">Mechvistaaservices</span>
                                        <span>â€¢</span>
                                        <span class="text-xs md:text-[1vw]">
                                            <%= new Date(blog.createdAt).toLocaleString('en-US', { month: 'short', day: 'numeric', year: 'numeric' }) %>
                                        </span>
                                    </div>
                                    
                                    <!-- Blog Title -->
                                    <h2 class="text-xl md:text-3xl font-semibold mt-2 text-[#322D23]">
                                        <%= blog.title %>
                                    </h2>
                                    
                                    <!-- Blog Description -->
                                    <h2 style="font-family: headm;" class="text-gray-800 text-base md:text-2xl mt-3">
                                        <%- blog.description.substring(0, 260) %><span class="text-blue-500">..more</span>
                                    </h2>
                                </div>
    
                                <!-- Blog Stats -->
                                <div class="flex border-t border-[#322D23] pt-3 justify-between items-center text-gray-500 text-sm mt-3">
                                    <div class="flex items-center space-x-3">
                                        <span class="text-lg md:text-xl text-[#332E24]">
                                            <i class="ri-eye-line"></i> <%= blog.views %> views
                                        </span>
                                        <span class="text-lg md:text-xl text-[#332E24]">
                                            <i class="ri-chat-3-line"></i> <%= blog.comments.length %> comments
                                        </span>
                                    </div>
                                    <button class="flex items-center text-lg md:text-xl text-[#332E24]">
                                        <i class="ri-heart-line"></i> <%= blog.likes.length %> Likes
                                    </button>
                                </div>
                            </div>
                        </div>
                    </a>
                <% }) %>
            <% } else { %>
                <h2 class="text-xl text-[#322D23] text-center">No blogs yet</h2>
            <% } %>
        </div>
    </div>
    
   </div>
